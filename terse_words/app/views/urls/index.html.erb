<div id="url_list">
  <% @urls.each do |url| %>
  <div class="url">
    <p>Full Url: <%= url.long_url %></p>
    <p>Shortened Url: <%= url.short_url %></p>
    <p><%= link_to("Edit", edit_url_path(url)) %></p>
    <p><%= link_to("Delete", url_path(url), method: :delete)%></p>
  </div>
  <% end %>
</div>